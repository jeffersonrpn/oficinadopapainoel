-- funcoes auxiliares
function print_center(text, y)
	local width = #text * 4
	local x = (128 - width) / 2
	print(text, x, y)
end

function print_r(text, y)
	local width = #text * 4
	local x = 128 - width
	print(text, x, y)
end
