-- movimento
local last_move = ""
local dash_delay = 20
function move(o)
	-- ultima posicao
	local lx = o.x
	local ly = o.y
	
	if btnp(üÖæÔ∏è) and dash_delay <= 0 then
		if last_move == "up" then
			o.y -= o.dash
		elseif last_move == "down" then
			o.y += o.dash
		elseif last_move == "left" then
			o.x -= o.dash
			o.f = true
		elseif last_move == "right" then
			o.x += o.dash
			o.f = false
		end
		dash_delay = 30
	else
		if btn(‚¨ÜÔ∏è) then
			o.y -= 1
			last_move = "up"
		end
		if btn(‚¨áÔ∏è) then
		 o.y += 1
		 last_move = "down"
		end
		if btn(‚¨ÖÔ∏è) then
			o.x -= 1
			o.f = true
			last_move = "left"
		end
		if btn(‚û°Ô∏è) then
			o.x += 1
			o.f = false
			last_move = "right"
		end
		if dash_delay > 0 then
			dash_delay -= 1
		end
	end
	
	-- se colide
	if collide_left(o) or collide_right(o) then
		o.x = lx
	end
	if collide_top(o) or collide_bottom(o) then
		o.y = ly
	end
end

function collide_left(o)
	local x1 = o.x/8
	local y1 = o.y/8
	local y2 = (o.y+7)/8
	
	local a = fget(mget(x1, y1), 0)
	local c = fget(mget(x1, y2), 0)
	
	return a or c
end

function collide_right(o)
	local x2 = (o.x+7)/8
	local y1 = o.y/8
	local y2 = (o.y+7)/8
	
	local b = fget(mget(x2, y1), 0)
	local d = fget(mget(x2, y2), 0)
	
	return b or d
end

function collide_top(o)
	local x1 = o.x/8
	local y1 = o.y/8
	local x2 = (o.x+7)/8
	
	local a = fget(mget(x1, y1), 0)
	local b = fget(mget(x2, y1), 0)
	
	return a or b
end

function collide_bottom(o)
	local x1 = o.x/8
	local y2 = (o.y+7)/8
	local x2 = (o.x+7)/8
	
	local c = fget(mget(x1, y2), 0)
	local d = fget(mget(x2, y2), 0)
	
	return c or d
end

function collide(o)
	local x1 = o.x/8
	local y1 = o.y/8
	local x2 = o.x+7/8
	local y2 = o.y+7/8
	
	local a = fget(mget(x1, y1), 0)
	local b = fget(mget(x2, y1), 0)
	local c = fget(mget(x1, y2), 0)
	local d = fget(mget(x2, y2), 0)
	
	return a or b or c or d
end

-- animacao andando
function walk(o, defaults)
	if defaults.animation_timer < 3 then
		defaults.animation_timer += 1
	else
		if o.sprite == defaults.player_stand_sprites[1] then
			o.sprite = defaults.player_stand_sprites[2]
		else
			o.sprite = defaults.player_stand_sprites[1]
		end
		defaults.animation_timer = 0
	end
end
