function init_menu()
	menu={
		options={
			"iniciar jogo",
			"creditos"
		},
		opt = 1,
		holding={
			down = false,
			up = false,
			x = false,
			z = false
		}
	}
end

function update_menu()
	if btnp(‚¨ÜÔ∏è) and not menu.holding.up then
		menu.opt = (menu.opt - 2) % #menu.options + 1
		menu.holding.up = true
	elseif not btn(‚¨ÜÔ∏è) then
		menu.holding.up = false
	end
	
	if btnp(‚¨áÔ∏è) and not menu.holding.down then
		menu.opt = menu.opt % #menu.options + 1
		menu.holding.down = true
	elseif not btn(‚¨áÔ∏è) then
		menu.holding.down = false
	end
	
	if btnp(‚ùé) then
		if menu.opt == 1 then
			-- iniciar jogo
			change_state("game")
		elseif menu.opt == 2 then
			-- creditos
			change_state("credits")
		end
	end
end

function draw_menu()
	cls()
	map(16)
	for i, option in ipairs(menu.options) do
		if i == menu.opt then
			print_center("> " .. option .. " <", 74 + i * 12)
		else
			print_center(option, 74 + i * 12)
		end
	end
end
