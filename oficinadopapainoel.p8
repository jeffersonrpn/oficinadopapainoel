-- definicao dos estados
local states={
	menu={
		update=update_menu,
		draw=draw_menu
	},
	credits={
		update=update_credits,
		draw=draw_credits
	},
	game={
		update=update_game,
		draw=draw_game
	}
}
local state = states.menu

function change_state(s)
	state = states[s]
	_update = state.update
	_draw	=	state.draw
end

function _init()
	init_menu()
	init_credits()
	init_game()
	init_toys()
	state = states.menu
end

function _update()
	state.update()
end

function _draw()
	state.draw()
end
