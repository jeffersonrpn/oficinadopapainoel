-- brinquedos
function init_toys()
	pieces = {
		{pa=42, pb=58, h=13, ont=45},
		{pa=43, pb=58, h=14, ont=46},
		{pa=44, pb=58, h=15, ont=47},
	}
	toys = {}
	toysp = {}

	for i = 1, 3 do
		local s = pieces[flr(rnd(#pieces)) + 1]
 	add(toys, {
 		x = 128 + (i - 1) * 60,
 		y = 32,
 		sprite = s,
			visible = true
 	})
 	add(toysp, {
 		x = 128 + (i - 1) * 60,
 		y = 40,
 		sprite = s,
			visible = true
 	})
	end
end

function update_toys()
	local s = pieces[flr(rnd(#pieces)) + 1]
	for i, t in ipairs(toys) do
			t.x -= 1
		if t.x + 7 <= 0 then
			t.x = toys[#toys].x + (i * 60)
			t.sprite = s
			t.visible = true
 	end
 end
	for i, t in ipairs(toysp) do
			t.x -= 1
		if t.x + 7 <= 0 then
			t.x = toysp[#toysp].x + (i * 60)
			t.sprite = s
			t.visible = true
 	end
 end
end

