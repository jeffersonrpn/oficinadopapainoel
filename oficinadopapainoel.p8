-- state: game
function init_game()
	defaults={
		factory_time = 1800, -- frames
		animation_timer = 30,
		player_stand_sprites={1,2},
		tread_animation_timer = 30,
		tread_sprites={27,28},
		tablea = {65,66,67},
		tableb = {81,82,83},
		tablenone = 39
	}

	player={
		sprite = defaults.player_stand_sprites[1],
		speed = 1,
		x = 60,
		y = 60,
		f = false,
		dash = 8,
		over = 0,
		holditem = {
				hold = false,
				sprite = {}
		}
	}
	
	tread={
		sprite = defaults.tread_sprites,
		x = 0,
		y = 40
	}
	
	tablea_item = {
		{haspiece=false,piece={}},
		{haspiece=false,piece={}},
		{haspiece=false,piece={}}
	}
	tableb_item = {}
	
	work={
		day = 1,
		money = 0,
		score = 0,
		factory_time = defaults.factory_time,
		cycle = "day_start",
		calculated_score = false,
	}
end

function update_game()
	update_toys()
	if work.cycle == "day_start" then
		-- etapa de inicio do dia
		player.holditem.hold = false
		for i,t in ipairs(toys) do
			local s = pieces[flr(rnd(#pieces)) + 1]
		 t.x = 128 + (i - 1) * 60
	 	t.sprite = s
		 toysp[i].x = 128 + (i - 1) * 60
		 toysp[i].sprite = s
		end
		if btnp(‚ùé) then
			work.cycle = "working"
		end
	elseif work.cycle == "working" then
		-- expediente
		work.factory_time -= 1
		
		move(player)
		walk(player, defaults)
		treadmill(tread, defaults)
		interact(player, toysp, defaults)
		
		if work.factory_time <= 0 then
			work.cycle = "day_end"
		end
	elseif work.cycle == "day_end" then
		-- etapa do fim do dia
		if not work.calculated_score then
			work.day += 1
			work.factory_time = defaults.factory_time
			work.score += 100
			work.calculated_score = true
			
			player.x = 63
			player.y = 63
			player.f = false
		end
		
		if btn(‚ùé) then
			work.cycle = "day_start"
			work.calculated_score = false
		end
	end	
end

function draw_game()
	cls()
	if work.cycle == "day_start" then
		-- etapa de inicio do dia
		cls()
		print_center("dia " .. work.day, 16)
	elseif work.cycle == "working" then
		-- expediente
		cls()
		map()
		print("dia " .. work.day, 1, 122)
		print_r(tostring(work.factory_time), 122)
		
		-- esteira
		for i=1,16 do
			spr(tread.sprite, tread.x + ((i-1) * 8), tread.y)
		end
		
		-- pecas de brinquedos
		for t in all(toys) do
			if (t.visible) then
				spr(t.sprite.pa, t.x, t.y)
			end
		end
		for t in all(toysp) do
			spr(t.sprite.pb, t.x, t.y)
		end
		
		--jogador
		spr(player.sprite, player.x, player.y, 1, 1, player.f)
		if (player.holditem.hold) then
			spr(player.holditem.sprite.h, player.x + 6, player.y - 8, 1, 1)
		end
	elseif work.cycle == "day_end" then
		-- etapa do fim do dia
		cls()
		print_center("dia " .. work.day, 16)
		print_center("pontuacao " .. work.score, 24)
	end
end
